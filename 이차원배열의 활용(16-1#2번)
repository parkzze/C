//2차원 배열의 활용(349p)
#include <stdio.h>
int main(void){
	int arr[2][4] = { 1, 2, 3, 4, 5, 6, 7, 8 };
	int brr[4][2];
	for (int i = 0; i < 2; i++){
		for (int j = 0; j <4; j++){
			brr[j][i] = arr[i][j];
		}
	}
	for (int j = 0; j <4; j++)
	{
		for (int i = 0; i < 2; i++)
		{
			printf("%d ", brr[j][i]);
		}
		printf("\n");
	}
}
